{
    "format_version": "1.10.0",
    "animation_controllers": {
        "controller.animation.player.is_alive": {
            "initial_state": "alive",
            "states": {
                "alive": {
                    "on_entry": [
                        "/tag @s remove is_dead"
                    ],
                    "transitions": [
                        {
                            "dead": "!query.is_alive"
                        }
                    ]
                },
                "dead": {
                    "on_entry": [
                        "/tag @s add is_dead"
                    ],
                    "transitions": [
                        {
                            "alive": "query.is_alive"
                        }
                    ]
                }
            }
        },
        "controller.animation.player.is_moving": {
